{
    "name": "clevyr/nova-help",
    "description": "A Laravel Nova help field.",
    "keywords": [
        "laravel",
        "nova",
        "help"
    ],
    "authors": [
        {
            "name": "Alessandro Benoit",
            "email": "alessandro@comodolab.it"
        },
        {
          "name": "Anthony Protano",
          "email": "anthony@clevyr.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "illuminate/support": ">=11.0",
        "laravel/nova": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Clevyr\\Nova\\Fields\\Help\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Clevyr\\Nova\\Fields\\Help\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
